<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 2.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd">
<hibernate-mapping>
	<class name="com.whir.ezoffice.wxlocation.po.WxWorkAttendanceAddressPO" table="WX_WORK_ATTENDANCE_ADDRESS" schema="EZOFFICE">
		<id name="id" column="ID">
			<generator class="native"/>
		</id>
		<property name="wxId" column="WX_ID"/>
		<property name="presentTime" column="PRESENTTIME"/>
		<property name="presentAddress" column="PRESENTADDRESS"/>
		<property name="longItude" column="LONGITUDE"/>
		<property name="latItude" column="LATITUDE"/>
		<property name="picture" column="PICTURE"/>
		<property name="remark" column="REMARK"/>
	</class>
</hibernate-mapping>